    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/roboto_medium"
        android:paddingLeft="24dp"
        android:paddingTop="14dp"
        android:paddingRight="24dp"
        android:paddingBottom="10dp"
        android:text="OBD + GPS"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/button2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline5" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:fontFamily="@font/roboto_medium"
        android:gravity="center"
        android:paddingLeft="24dp"
        android:paddingTop="14dp"
        android:paddingRight="24dp"
        android:paddingBottom="10dp"
        android:text="GPS Only"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button"
        app:layout_constraintTop_toTopOf="@+id/button" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:background="#167FC1"
        app:layout_constraintEnd_toEndOf="@+id/button"
        app:layout_constraintStart_toStartOf="@+id/button"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:background="#167FC1"
        app:layout_constraintEnd_toEndOf="@+id/button2"
        app:layout_constraintStart_toStartOf="@+id/button2"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <LinearLayout
        android:id="@+id/bannerBluetoothContainer"
        android:layout_width="92dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="vertical"
        android:padding="4dp"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@+id/bannerOBDAdapterContainer"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17">

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="@dimen/dashboard_selector"
                android:layout_height="@dimen/dashboard_selector"
                android:layout_gravity="center"
                android:background="@drawable/btn_dashboard_circle"
                android:padding="10dp"
                android:src="@drawable/ic_bluetooth_black_24dp"
                android:visibility="visible" />

            <ImageView
                android:id="@+id/errorImageBluetooth"
                android:layout_width="@dimen/dashboard_selector"
                android:layout_height="@dimen/dashboard_selector"
                android:layout_gravity="center"
                android:background="@drawable/btn_dashboard_circle"
                android:padding="10dp"
                android:src="@drawable/bluetooth_error"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/okImageBluetooth"
                android:layout_width="@dimen/dashboard_selector"
                android:layout_height="@dimen/dashboard_selector"
                android:layout_gravity="center"
                android:background="@drawable/btn_dashboard_connected_circle"
                android:padding="10dp"
                android:src="@drawable/bluetooth_connected"
                android:tint="@color/common_google_signin_btn_text_dark_focused"
                android:visibility="gone" />


        </FrameLayout>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/bannerOBDAdapterContainer"
        android:layout_width="93dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="vertical"
        android:padding="4dp"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@+id/bannerGPSContainer"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/bannerBluetoothContainer"
        app:layout_constraintTop_toTopOf="@+id/bannerBluetoothContainer">

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="@dimen/dashboard_selector"
                android:layout_height="@dimen/dashboard_selector"
                android:layout_gravity="center"
                android:background="@drawable/btn_dashboard_circle"
                android:padding="10dp"
                android:src="@drawable/obd"
                android:tint="#000000"
                android:visibility="visible" />

            <ImageView
                android:id="@+id/errorImageOBDAdapter"
                android:layout_width="@dimen/dashboard_selector"
                android:layout_height="@dimen/dashboard_selector"
                android:layout_gravity="center"
                android:background="@drawable/btn_dashboard_circle"
                android:padding="10dp"
                android:src="@drawable/obd_nc"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/okImageOBDAdapter"
                android:layout_width="@dimen/dashboard_selector"
                android:layout_height="@dimen/dashboard_selector"
                android:layout_gravity="center"
                android:background="@drawable/btn_dashboard_connected_circle"
                android:padding="10dp"
                android:src="@drawable/obd_connected"
                android:tint="@color/common_google_signin_btn_text_dark_focused"
                android:visibility="gone" />

        </FrameLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bannerGPSContainer"
        android:layout_width="93dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="vertical"
        android:padding="4dp"
        app:layout_constraintEnd_toStartOf="@+id/bannerCarContainer"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/bannerOBDAdapterContainer"
        app:layout_constraintTop_toTopOf="@+id/bannerOBDAdapterContainer">

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="@dimen/dashboard_selector"
                android:layout_height="@dimen/dashboard_selector"
                android:layout_gravity="center"
                android:background="@drawable/btn_dashboard_circle"
                android:padding="10dp"
                android:src="@drawable/gps"
                android:tint="#000000"
                android:visibility="visible" />

            <ImageView
                android:id="@+id/errorImageGPS"
                android:layout_width="@dimen/dashboard_selector"
                android:layout_height="@dimen/dashboard_selector"
                android:layout_gravity="center"
                android:background="@drawable/btn_dashboard_circle"
                android:padding="10dp"
                android:src="@drawable/gps_off"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/okImageGPS"
                android:layout_width="@dimen/dashboard_selector"
                android:layout_height="@dimen/dashboard_selector"
                android:layout_gravity="center"
                android:background="@drawable/btn_dashboard_connected_circle"
                android:padding="10dp"
                android:src="@drawable/gps_connected"
                android:tint="@color/common_google_signin_btn_text_dark_focused"
                android:visibility="gone" />

        </FrameLayout>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/bannerCarContainer"
        android:layout_width="93dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="vertical"
        android:padding="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/bannerGPSContainer"
        app:layout_constraintTop_toTopOf="@+id/bannerGPSContainer">

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="@dimen/dashboard_selector"
                android:layout_height="@dimen/dashboard_selector"
                android:layout_gravity="center"
                android:background="@drawable/btn_dashboard_circle"
                android:padding="10dp"
                android:src="@drawable/ic_drive_eta_black_24dp"
                android:tint="@color/common_google_signin_btn_text_dark_focused"
                android:visibility="visible" />

            <ImageView
                android:id="@+id/errorImageCar"
                android:layout_width="@dimen/dashboard_selector"
                android:layout_height="@dimen/dashboard_selector"
                android:layout_gravity="center"
                android:background="@drawable/btn_dashboard_circle"
                android:padding="10dp"
                android:src="@drawable/ic_drive_eta_black_24dp"
                android:tint="@color/common_google_signin_btn_text_dark_focused"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/okImageCar"
                android:layout_width="@dimen/dashboard_selector"
                android:layout_height="@dimen/dashboard_selector"
                android:layout_gravity="center"
                android:background="@drawable/btn_dashboard_connected_circle"
                android:padding="10dp"
                android:src="@drawable/ic_drive_eta_black_24dp"
                android:tint="@color/common_google_signin_btn_text_dark_focused"
                android:visibility="gone" />

        </FrameLayout>

    </LinearLayout>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="bannerGPSContainer, bannerOBDAdapterContainer, bannerCarContainer, bannerBluetoothContainer"
        tools:layout_editor_absoluteY="341dp" />

    <androidx.core.widget.NestedScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/barrier2">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/disableChangingParametersLayout1"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:background="@color/envirocar_color_divider_grey"
                android:clickable="true"
                android:elevation="5dp"
                android:orientation="horizontal"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="@+id/dash_board_view_car_selection1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/dashboard_view_obd_selection1" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/dashboard_view_obd_selection1"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_marginStart="24dp"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="24dp"
                android:layout_marginRight="24dp"
                android:clickable="true"
                android:focusable="true"
                android:orientation="horizontal"
                android:paddingTop="@dimen/spacing_medium"
                android:paddingBottom="@dimen/spacing_medium"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/dashboard_view_obd_selection_img"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/spacing_large"
                    android:layout_marginLeft="@dimen/spacing_large"
                    android:src="@drawable/ic_bluetooth_searching_black_24dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <TextView
                    android:id="@+id/dash_board_view_obd_selection_text2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="28dp"
                    android:layout_marginLeft="28dp"
                    android:layout_marginTop="2dp"
                    android:fontFamily="@font/roboto"
                    android:singleLine="true"
                    android:text="Car Attributes"
                    android:textColor="#000000"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/dashboard_view_obd_selection_img"
                    app:layout_constraintTop_toBottomOf="@+id/dash_board_view_obd_selection_text1" />

                <TextView
                    android:id="@+id/dash_board_view_obd_selection_text1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="2dp"
                    android:fontFamily="@font/roboto_medium"
                    android:singleLine="true"
                    android:text="No OBD Device Selected"
                    android:textColor="#000000"
                    android:textSize="18sp"
                    app:layout_constraintBottom_toTopOf="@+id/dash_board_view_obd_selection_text2"
                    app:layout_constraintStart_toStartOf="@+id/dash_board_view_obd_selection_text2"
                    app:layout_constraintTop_toTopOf="parent" />


            </androidx.constraintlayout.widget.ConstraintLayout>


